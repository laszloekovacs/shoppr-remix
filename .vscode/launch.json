{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "remix run",
			"command": "npm run dev",
			"request": "launch",
			"type": "node-terminal",
			"cwd": "${workspaceFolder}"
		},
		{
			"name": "remix attach",
			"processId": "${command:PickProcess}",
			"request": "attach",
			"skipFiles": ["<node_internals>/**"],
			"type": "node"
		}
	],
	"compounds": []
}
